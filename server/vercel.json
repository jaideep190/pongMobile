{
    "version": 2,
    "name": "your-server",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "PORT": "@port"
    },
    "regions": ["all"]
  }